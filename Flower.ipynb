   "source": [
    "import turtle\n",
    "t = turtle.Turtle()\n",
    "s = turtle.Screen()\n",
    "s.bgcolor('black')\n",
    "t.speed(0)\n",
    "radius = 60\n",
    "t.pensize(2)\n",
    "color = ['red','white','red']\n",
    "for x in range(12):\n",
    "    t.color(color[x%3])\n",
    "for i in range(8):\n",
    "    t.circle(radius)\n",
    "    t.right(60)\n",
    "radius = radius + 4"
